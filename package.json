{
  "name": "cacao-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "view-install": "npm install --prefix cacao-app-view",
    "view": "npm start --prefix cacao-app-view",
    "dev": "concurrently \"npm run server\" \"npm run view\"",
    "test": "nyc --reporter=text mocha && mocha --timeout 10000",
    "test-single": "mocha $1 --timeout 10000"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "request": "^2.88.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
